{
  "version": 2,
  "builds": [
    {
      "src": "api/trigger-workflow.js",
      "use": "@vercel/node"
    },
    {
      "src": "pages/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/trigger-workflow",
      "dest": "/api/trigger-workflow.js"
    },
    {
      "src": "/dashboard",
      "dest": "/pages/rh_dashboard.html"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/pages/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/pages/index.html"
    }
  ]
}